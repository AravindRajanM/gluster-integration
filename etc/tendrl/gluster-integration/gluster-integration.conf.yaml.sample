etcd_port: 2379
etcd_connection: 0.0.0.0

# Enable etcd username/password based auth, disable if using CA Cert/Client cert based TLS auth options
#etcd_username: root
#etcd_password: replace_your_password_here

logging_socket_path: /var/run/tendrl/message.sock
sync_interval: 180

# Directory to store tendrl managed brick mounts
gluster_bricks_dir: /tendrl_gluster_bricks
with_internal_profiling: False
